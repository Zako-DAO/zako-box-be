{
  "name": "zako-box-be",
  "type": "module",
  "scripts": {
    "build": "bun build ./src/index.ts --compile --outfile zako-box-be",
    "lint": "bun --bun eslint .",
    "lint:fix": "bun --bun eslint . --fix",
    "dev": "bun run --hot src/index.ts",
    "typecheck": "bun --bun tsc --noEmit",
    "db:generate": "bun --bun drizzle-kit generate",
    "db:migrate": "bun --bun drizzle-kit migrate"
  },
  "dependencies": {
    "@guiiai/logg": "^1.1.0",
    "drizzle-orm": "^0.44.6",
    "hono": "^4.10.0",
    "pg": "^8.16.3",
    "viem": "^2.38.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.0.0",
    "@types/pg": "^8.15.5",
    "bun-types": "^1.3.0",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9.37.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
